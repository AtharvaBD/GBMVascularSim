<?xml version='1.0' encoding='UTF-8'?>
<MorpheusModel version="4">
    <Description>
        <Details></Details>
        <Title></Title>
    </Description>
    <Space>
        <Lattice class="square">
            <Neighborhood>
                <Order>optimal</Order>
            </Neighborhood>
            <Size symbol="size" value="300, 300, 0.0"/>
            <BoundaryConditions>
                <Condition type="periodic" boundary="x"/>
                <Condition type="periodic" boundary="y"/>
            </BoundaryConditions>
        </Lattice>
        <SpaceSymbol symbol="space"/>
    </Space>
    <Time>
        <StartTime value="0"/>
        <StopTime value="500"/>
        <TimeSymbol symbol="time"/>
        <SaveInterval value="500"/>
    </Time>
    <Analysis>
        <ModelGraph include-tags="#untagged" format="dot" reduced="false"/>
        <Gnuplotter time-step="50">
            <Plot>
                <Field symbol-ref="chemical"/>
                <Cells value="cell.type"/>
            </Plot>
            <Terminal name="png"/>
        </Gnuplotter>
    </Analysis>
    <Global>
        <Field symbol="chemical" value="exp( - (sqrt((space.x-cx)^2 + (space.y-cy)^2) - R)^2 / (2*W^2))"/>
        <Constant symbol="R" value="40"/>
        <Constant symbol="W" value="15"/>
        <Constant symbol="cx" value="size.x/2"/>
        <Constant symbol="cy" value="size.y/2"/>
    </Global>
    <CellTypes>
        <CellType name="cell" class="biological">
            <VolumeConstraint target="2100" strength="1"/>
            <SurfaceConstraint target="2.2" strength="10" mode="aspherity"/>
            <Chemotaxis strength="1000" field="chemical"/>
            <LengthConstraint target="1.5" strength="1" mode="length"/>
        </CellType>
    </CellTypes>
    <CellPopulations>
        <Population type="cell" size="1">
            <TIFFReader filename="C:/Users/birat/Desktop/GBMVascularSim/endothelial_cells.tif"/>
        </Population>
    </CellPopulations>
    <CPM>
        <Interaction>
            <Contact type2="cell" type1="cell" value="0.5">
                <HeterophilicAdhesion adhesive1="1" strength="1" adhesive2="1"/>
            </Contact>
        </Interaction>
        <ShapeSurface scaling="norm">
            <Neighborhood>
                <Order>optimal</Order>
            </Neighborhood>
        </ShapeSurface>
        <MonteCarloSampler stepper="edgelist">
            <MCSDuration value="0.3"/>
            <MetropolisKinetics temperature="1"/>
            <Neighborhood>
                <Order>optimal</Order>
            </Neighborhood>
        </MonteCarloSampler>
    </CPM>
</MorpheusModel>
